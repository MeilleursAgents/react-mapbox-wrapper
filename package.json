{
  "name": "react-mapbox-wrapper",
  "version": "0.1.0",
  "description": "Mapbox GL Javascript wrapper for ReactJS",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MeilleursAgents/react-mapbox-wrapper.git"
  },
  "main": "build/index.js",
  "scripts": {
    "peers": "npm-install-peers",
    "format": "prettier-eslint --write",
    "lint": "eslint --fix --ext .jsx,.js src",
    "test": "NODE_PATH=./src nyc --all mocha --opts ./mocha.opts \"./src/**/*.test.js\" \"./src/**/*.test.jsx\"",
    "test:file": "NODE_PATH=./src mocha --opts ./mocha.opts",
    "doc": "esdoc -c esdoc.json",
    "start": "webpack --watch",
    "build": "webpack -p",
    "ci": "npm run format && npm run lint && npm run test && npm run doc && npm run build"
  },
  "keywords": [
    "react",
    "component",
    "library",
    "mapboxgl-js",
    "webpack"
  ],
  "author": "MeilleursAgents",
  "license": "MIT",
  "homepage": "http://meilleursagents.github.io/react-mapbox-wrapper",
  "engines": {
    "node": ">=8.0.0"
  },
  "peerDependencies": {
    "@turf/circle": "^6.0.0",
    "mapbox-gl": "^0.46.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-template": "6.26.0",
    "babel-types": "6.26.0",
    "chai": "4.1.2",
    "css-loader": "1.0.0",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.5.0",
    "esdoc": "1.1.0",
    "esdoc-coverage-plugin": "1.1.0",
    "esdoc-ecmascript-proposal-plugin": "1.0.0",
    "esdoc-es7-plugin": "0.0.3",
    "esdoc-jsx-plugin": "1.0.0",
    "esdoc-plugin-require-coverage": "0.1.2",
    "esdoc-publish-html-plugin": "1.1.2",
    "esdoc-standard-plugin": "1.0.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-config-prettier": "2.9.0",
    "eslint-import-resolver-webpack": "0.8.3",
    "eslint-loader": "2.1.0",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "5.1.0",
    "eslint-plugin-react": "7.10.0",
    "ignore-styles": "5.0.1",
    "jsdom": "12.0.0",
    "mocha": "5.2.0",
    "npm-install-peers": "1.2.1",
    "nyc": "12.0.2",
    "prettier-eslint-cli": "4.7.1",
    "sinon": "5.1.1",
    "sinon-chai": "3.1.0",
    "style-loader": "0.23.0",
    "webpack": "4.12.0",
    "webpack-cli": "3.0.3",
    "webpack-node-externals": "1.7.2"
  },
  "bugs": {
    "url": "https://github.com/MeilleursAgents/react-mapbox-wrapper/issues"
  },
  "directories": {
    "example": "examples"
  }
}
